{
    "require": {
        "php": "^7.0",

        "fzaninotto/faker": "v1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.7"
    },
    "autoload": {
        "psr-4": {
            "Convelio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "bin-dir": "bin"
    }
}
